    <Action Name="A_PowerOn" Id="{3be612a8-046c-45b5-ad28-b2fb90dcb4f1}">
      <Implementation>
        <ST><![CDATA[fb_ton(PT:=T#0.5S);
CASE PowerState OF
	0:	(*FB_Power_On*)
		FB_Power(On:=TRUE);
		fb_ton.IN:=TRUE;
		PowerState:=1;
	1:	(*wait*)
		FB_power();
		IF NOT FB_power.OnStatus THEN		(*FB_Power_OnÃ»ÓÐÖ´ÐÐÍê£¬wait£¬³¬Ê±FB Error*)
			IF NOT fb_ton.Q THEN
				PowerState:=1;
			ELSE
				ErrorID:=1;
				PowerState:=99;
			END_IF
		ELSE
			PowerState:=88;
		END_IF
	88:	(*All Done*)
		btn_PowerOn:=FALSE;
		PowerState:=0;
		Error:=FALSE;
		ErrorID:=0;
		Busy:=FALSE;
		InitDone:=TRUE;
		FB_power.On:=FALSE;
		fb_ton.IN:=FALSE;
	99:
		btn_PowerOn:=FALSE;
		PowerState:=99;
		Error:=TRUE;
		Busy:=FALSE;
		InitDone:=FALSE;
		FB_Power.On:=FALSE;
		fb_ton.IN:=FALSE;
		LED_Error:=TRUE;
END_CASE]]></ST>
      </Implementation>
    </Action>
